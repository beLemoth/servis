// PRODUCTS SECTION

section.products
	.container-wrap
		h2 Наша продукция

		.products-wrap
			-
			each item, name in prodList
				each sizes, desc in item
					.prod-item-wrap
						a.product-item(href="products.html#"+name)
							.product-item__img
								img.product-item__img_item(src="./assets/img/prod-"+ name + ".png" alt=""+ desc)
							.product-item__name
								.product-item__name-text= desc
